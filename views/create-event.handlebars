<div class="column">
  <div>
      <label style="margin-left:10px;font:normal 16px arial;">{{send.address}}</label>
    </div>
      <div>
      <img src="/images/{{send.like}}.png" style="margin-left:10px;width:30px" /><label style="display:inline-block;font:bold 16px arial;margin-left:10px;">{{send.reviews}}</label>
    </div>

  <h2 class="title">Register Event</h2><hr />
  <form class="event-form">
    <div>
      <label for="type-event">Type:</label>
      <select id="type-event" style="width: 100%; height:41px; border-radius:6px"><option value=''>Choose</option><option value='1'>Xmas Lights</option><option value='2'>Halloween</option><option value='3'>Garage Sale</option></select>
    </div>
    <div>
      <label for="startdate-event">Start Date:</label>
      <input type="text" id="startdate-event" class="datepicker">
    </div>
    <div>
      <label for="enddate-event">End Date:</label>
      <input type="text" id="enddate-event" class="datepicker" >
    </div>
     <div>
      <label for="starttime-event">Start Time:</label>
      <input type="text" id="starttime-event" class="timepicker" />
    </div>   
     <div>
      <label for="endtime-event">End Time:</label>
      <input type="text" id="endtime-event" class="timepicker" />
    </div>   
    <div>
    <div>
      <label for="image-event">Photo:</label>
      <input id="image-event" type='file' />
      <br><img id="myImg" src="/images/house_image.png" style="width:100%">
      <br/><br/>
    </div>
      <button type="submit" class="button">Register</button>
    </div>
  </form>
   <a href="/" ><button class="button">Cancel</button>
</div>

<script src="/js/register-event.js"></script>
